{
  "builds": [
    {
      "src": "frontend/package.json",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/(.*).js",
      "dest": "/frontend/$1.js"
    },
    {
      "src": "/(.*).css",
      "dest": "/frontend/$1.css"
    },
    {
      "src": "/(.*)",
      "dest": "/frontend/index.html"
    }
  ]
}
